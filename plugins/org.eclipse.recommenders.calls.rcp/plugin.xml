<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.recommenders.completion.rcp.sessionprocessors">
      <processor
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.calls.rcp.CallCompletionSessionProcessor"
            icon="icons/view16/braces.gif"
            id="org.eclipse.recommenders.calls.rcp.processor1"
            name="Call Recommender 2">
      </processor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.recommenders.rcp.category.preferences"
            class="org.eclipse.recommenders.calls.rcp.PreferencePage"
            id="org.eclipse.recommenders.calls.rcp.preferencepage"
            name="Calls 2.0">
      </page>
   </extension>
   <extension
         point="org.eclipse.equinox.preferences.preferences">
      <initializer
            class="org.eclipse.recommenders.calls.rcp.wiring.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.recommenders.injection.modules">
      <module
            class="org.eclipse.recommenders.calls.rcp.wiring.CallsRcpModule">
      </module>
   </extension>

</plugin>
