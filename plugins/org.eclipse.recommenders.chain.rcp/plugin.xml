<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="ChainCompletionProposalComputer"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <javaCompletionProposalComputer
            activate="true"
            categoryId="org.eclipse.recommenders.chain.rcp.category"
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.chain.rcp.ChainCompletionProposalComputer">
      </javaCompletionProposalComputer>
   </extension>
   <extension
         id="org.eclipse.recommenders.chain.rcp.category"
         name="%extension.name"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <proposalCategory
            icon="icons/view16/chain.gif">
      </proposalCategory>
   </extension>
	<extension
         point="org.eclipse.recommenders.injection.modules">
         <module
            class="org.eclipse.recommenders.internal.chain.rcp.ChainRcpModule">
      </module>
	</extension>
	<extension
		point="org.eclipse.ui.preferencePages">
		<page
			category="org.eclipse.recommenders.rcp.category.preferences"
			class="org.eclipse.recommenders.internal.chain.rcp.ChainPreferencePage"
			id="org.eclipse.recommenders.chain.rcp.ChainPreferencePage"
			name="%page.name">
		</page>
	</extension>
	<extension
		point="org.eclipse.core.runtime.preferences">
		<initializer
			class="org.eclipse.recommenders.internal.chain.rcp.ChainPreferenceInitializer">
		</initializer>
	</extension>
 <extension
       point="org.eclipse.jdt.ui.quickAssistProcessors">
    <quickAssistProcessor
          class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.chain.rcp.QuickAssistProcessor"
          id="org.eclipse.recommenders.chain.rcp.quickAssistProcessor"
          name="Chain Assignments Quick Assists">
    </quickAssistProcessor>
 </extension>
</plugin>
